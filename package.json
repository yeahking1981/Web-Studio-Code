  },
  "homepage": "https://github.com/yelloxing/Web-Studio-Code#readme",
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@rollup/plugin-commonjs": "^11.1.0",
    "@rollup/plugin-node-resolve": "^7.1.3",
    "@yelloxing/core.js": "^0.3.7",
    "babelify": "^10.0.0",
